create database [Musica];
use [Musica] 
create table country
(
 [ID] int not null primary key identity,
 [Name] nvarchar(100) not null unique,
)
create table publisher
(
[ID] int not null primary key identity,
[Name] nvarchar(100) not null unique,
[CoutryID]int not null references country(ID),
)
create table style
(
[ID] int not null primary key identity,
[Name] nvarchar(100) not null unique ,
) 
create table Performer
(
[ID] int not null primary key identity,
[Name] nvarchar(100) not null ,
[Surname]nvarchar(100) not null,
)
create table AudioDisk
(
[ID]int not null primary key identity ,
[Name] nvarchar(100) not null unique,
[PerformerID]int not null references Performer(ID),
[PublisherID]int not null references Publisher(ID),
[StyleID]int not null references Style(ID),
[DateOfRealease]date not null ,
)
create table songs
(
[ID]int not null primary key identity,
[Name] nvarchar(100) not null unique,
[AudioDiskID]int not null references  AudioDisk(ID),
)
